{
  "name": "mos-plugin-license",
  "version": "0.0.0-placeholder",
  "description": "A mos plugin for generating a license section",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha && npm run lint && mos test -x=\"test/**\"",
    "lint": "eslint lib/**/*.js index.js",
    "commit": "git-cz",
    "coverage": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha -- -R spec",
    "precoveralls": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage",
    "md": "mos -x=\"test/**\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mosjs/mos-plugin-license"
  },
  "keywords": [
    "mos-plugin",
    "license",
    "licence"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mosjs/mos-plugin-license/issues"
  },
  "homepage": "https://github.com/mosjs/mos-plugin-license#readme",
  "dependencies": {
    "file-exists": "^1.0.0",
    "markdownscript": "^1.2.0",
    "mdast-util-to-string": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "ghooks": "^1.2.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "mos": "^1.0.0",
    "mos-plugin-ejs": "^1.0.1",
    "mos-processor": "^1.1.1",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^2.6.1"
  },
  "engines": {
    "node": ">=4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js",
      "pre-commit": "npm test"
    }
  }
}
